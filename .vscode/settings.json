{
  "files.exclude": {
    "**/.git": false,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": false
  },
  "[handlebars]": {
    "editor.suggest.insertMode": "insert",
    "editor.indentSize": "tabSize",
    "editor.detectIndentation": false,
    "editor.tabSize": 2
  }
}
